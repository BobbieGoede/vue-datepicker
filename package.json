{
  "name": "@sum.cumo/vue-datepicker",
  "version": "1.0.0",
  "description": "A simple Vue.js datepicker component. Supports disabling of dates, inline mode, translations",
  "license": "Apache-2.0",
  "homepage": "https://github.com/sumcumo/vue-datepicker",
  "repository": {
    "type": "git",
    "url": "git@github.com:sumcumo/vue-datepicker.git"
  },
  "author": "sum.cumo GmbH",
  "keywords": [
    "vue",
    "datepicker",
    "date-picker",
    "calendar",
    "typeable"
  ],
  "main": "dist/vuejs-datepicker.js",
  "module": "dist/vuejs-datepicker.esm.js",
  "unpkg": "dist/vuejs-datepicker.min.js",
  "files": [
    "src",
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "prepare": "npm run clear && npm run build",
    "build": "babel-node scripts/build.js && babel-node scripts/build-locale.js",
    "clear": "rm -rf dist/*",
    "audit": "npm audit --registry=https://registry.npmjs.org",
    "serve": "rollup -c scripts/serve.js --watch",
    "lint": "eslint --ext .js,.vue src test/unit/specs",
    "test": "jest --config test/unit/jest.conf.js --coverage",
    "release": "standard-version",
    "prerelease": "standard-version --dry-run",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "standard-version": {
    "releaseCommitMessageFormat": "chore(release): {{currentTag}}"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "peerDependencies": {
    "vue": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.7.4",
    "@babel/node": "7.7.4",
    "@babel/plugin-transform-runtime": "7.7.4",
    "@babel/preset-env": "7.7.4",
    "@vue/test-utils": "1.0.0-beta.29",
    "autoprefixer": "9.7.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "chalk": "3.0.0",
    "cleave.js": "1.5.3",
    "core-js": "3.4.7",
    "eslint": "6.7.2",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-standard": "14.1.0",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-import-resolver-webpack": "0.11.1",
    "eslint-loader": "3.0.2",
    "eslint-plugin-compat": "3.3.0",
    "eslint-plugin-html": "6.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "23.1.1",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "eslint-plugin-vue": "6.0.1",
    "jest": "24.9.0",
    "jest-serializer-vue": "2.0.2",
    "node-sass": "4.13.0",
    "pre-commit": "1.2.2",
    "rollup": "1.27.8",
    "rollup-plugin-alias": "2.2.0",
    "rollup-plugin-buble": "0.19.8",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-livereload": "1.0.4",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-postcss": "2.0.3",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-serve": "1.0.1",
    "rollup-plugin-terser": "5.1.2",
    "rollup-plugin-vue": "5.1.4",
    "semver": "6.3.0",
    "standard-version": "7.0.1",
    "vee-validate": "2.2.14",
    "vue": "2.6.10",
    "vue-jest": "3.0.5",
    "vue-server-renderer": "2.6.10",
    "vue-template-compiler": "2.6.10",
    "vuepress": "1.2.0"
  }
}
